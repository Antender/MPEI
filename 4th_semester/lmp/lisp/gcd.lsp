(defun gcd! (a b)  (cond    ((= a b) a)    ((= 1 a) 1)    ((= 1 b) 1)    ((evenp a) (cond      ((evenp b) (* 2 (gcd! (/ a 2) (/ b 2))))      (T (gcd! (/ a 2) b))    ))    ((evenp b) (gcd! a (/ b 2)))    ((> a b) (gcd! (/ (- a b) 2) b))    (T (gcd! (/ (- b a) 2) a))  ))(defun gcd (a b)  (cond    ((= 0 a) (cond      ((= 0 b) 1)      (T a)    ))    ((= 0 b) b)    (T (gcd! (abs a) (abs b)))  ))